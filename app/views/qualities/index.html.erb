<h1>Calidades</h1>

<table>
	<thead>
		<tr>
			<th>Uri</th>
			<th>Sequence</th>
			<th>Name</th>
			<th></th>
			<th></th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		<% @qualities.each do |quality| %>
		<tr>
			<td><%= quality.rdf_uri %></td>
			<td><%= quality.quality_id %></td>
			<td><%= quality.quality_name %></td>
			<td><%= link_to 'Mostrar', rdf_quality_path(quality.id) %></td>
			<td><%= link_to 'Editar', edit_rdf_quality_path(quality.id) %></td>
			<td><%= link_to 'Destruír', destroy_rdf_quality_path(quality.id), confirm: 'Está seguro?' %></td>
		</tr>
		<% end %>
	<tbody>
</table>

<br />

<%= link_to 'Nueva Calidad', new_rdf_quality_path %>
